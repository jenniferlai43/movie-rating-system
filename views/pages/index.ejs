<!-- views/pages/index.ejs -->
<html>
	<head>
		<% include ../partials/head %>
	</head>
	<body>
		<% include ../partials/sidebar %>
		<ul>
			<% for(var i=0; i<list.length; i++){ %>
				<li><%= list[i].movie %></li>
				<ul>
					<li>Rate: <%= list[i].rate %></li>
					<li>Genre: <%= list[i].genre %></li>
					<li>Description: <%= list[i].description %></li>
					<li>Created at: <%= list[i].created_at %></li>
					<li>Updated at: <%= list[i].updated_at %></li>
				</ul>
			<% } %>
		</ul>
	</body>
</html>
