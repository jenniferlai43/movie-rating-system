<!-- views/pages/index.ejs -->
<!DOCTYPE html>
<html>
	<head>
		<% include ../partials/head %>
	</head>
	<body>
		<% include ../partials/header %>
		<% include ../partials/sidebar %>
		<div class = "movie-list">
			<ul>
				<% for(var i=0; i<list.length; i++){ %>
					<div class = movie_item>
						<ul id = "movie">
							<li id = "title"><%= list[i].movie %></li>
							<div class = "details">
								<li>Rate: <%= list[i].rate %></li>
								<li>Genre: <%= list[i].genre %></li>
								<li>Description: <%= list[i].description %></li>
								<li>Created at: <%= list[i].created_at %></li>
								<li>Updated at: <%= list[i].updated_at %></li>
							</div>
						</ul>
						<img src = "data:image/png;base64," / >
						<br />
					</div>
				<% } %>
			</ul>
		</div>
	</body>
	<script type="text/javascript" src="./movie-list.js"></script>
</html>
